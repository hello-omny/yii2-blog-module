{
    "name": "omny/yii2-blog-module",
    "description": "Yii2 blog module",
    "type": "yii2-extension",
    "keywords": [
        "yii2",
        "extension",
        "blog",
        "module"
    ],
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Omny",
            "email": "hello.omny@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "ext-json": "*",
        "yiisoft/yii2": "^2.0",
        "vova07/yii2-imperavi-widget": "^2.0",
        "2amigos/yii2-selectize-widget": "~1.0",
        "kartik-v/yii2-widget-datepicker": "^1.4",
        "2amigos/yii2-taggable-behavior": "~1.0",
        "kartik-v/yii2-widget-fileinput": "^1.0"
    },
    "require-dev": {
        "codeception/codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-faker": "*"
    },
    "autoload": {
        "psr-4": {
            "omny\\yii2\\blog\\module\\": "src/",
            "tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 1800
    },
    "repositories": [
      {
        "type": "composer",
        "url": "https://asset-packagist.org"
      }
    ]
}
